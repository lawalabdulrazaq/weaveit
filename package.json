    "@solana/web3.js": "^1.98.0",
    "axios": "^1.8.4",
    "bs58": "^6.0.0",
    "canvas": "^3.1.0",
    "dotenv": "^16.4.7",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.3",
    "node-gtts": "^2.0.2",
    "openai": "^4.92.1"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
